@model IEnumerable<FARApplication.Web.Models.FAR>


@{
    ViewData["Title"] = "Index";
}


<table class="table">
    <thead>
        <tr>
            <th>
                <span>FAR ID</span>

            </th>
            <th>
                <span>Creator Name</span>
            </th>
            <th>
                <span>Created On</span>

            </th>

            <th>

                <span>Status</span>
            </th>
            <th>
                <span>Summary</span>

            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink(item.RequestId,"GetFARResult","FAR", new { FARId = item.Id })
                    @*@Html.DisplayFor(modelItem => item.RequestId)*@
